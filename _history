ngeLifeBy(-1)\n        // Add visual and sound feedback when hit\n        myfox.startEffect(effects.blizzard, 200)\n        music.playTone(261, 100) // Play a low tone when hit\n        if (info.life() === 0) {\n            // Show game over screen with score and highscore\n            game.splash(\"Game Over! SCORE: \" + score, \"HIGHSCORE: \" + info.highScore())\n            game.gameOver(false)\n            gameOver = true\n        }\n    }\n})","README.md":" ","assets.json":"","pxt.json":"{\n    \"name\": \"Axe Hunt: Pixel Paws\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"device\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"assets.json\"\n    ],\n    \"targetVersions\": {\n        \"branch\": \"v2.0.48\",\n        \"tag\": \"v2.0.48\",\n        \"commits\": \"https://github.com/microsoft/pxt-arcade/commits/cb1bfd53862604a2129aba2eff909d3bec8826ed\",\n        \"target\": \"2.0.48\",\n        \"pxt\": \"11.3.51\"\n    },\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1746659517220,"editorVersion":"2.0.53","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables></variables><block type=\"pxt-on-start\" x=\"0\" y=\"0\"></block></xml>","main.ts":"scene.setBackgroundImage(img`\n    1111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111\n    1111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111\n    1111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111\n    1111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111\n   